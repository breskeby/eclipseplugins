<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.breskeby.eclipse.gradle.feature"
      label="Basic Gradle Support for Eclipse"
      version="0.1.0.qualifier"
      provider-name="rene@breskeby.com">

   <description url="%descriptionURL">
      %description
   </description>

   <copyright url="http://www.example.com/copyright">
      %copyright
   </copyright>

   <license url="%license.html">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.jface.text" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.editors" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.variables" version="3.2.200" match="greaterOrEqual"/>
      <import plugin="org.eclipse.debug.ui" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.debug" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.debug.ui" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.externaltools" version="3.2.0" match="compatible"/>
      <import plugin="org.eclipse.jdt.core" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.launching" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.codehaus.gradle" version="0.8.0" match="compatible"/>
      <import plugin="org.eclipse.ui.console" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="com.ibm.icu"/>
      <import plugin="org.apache.ant"/>
      <import feature="org.codehaus.groovy.eclipse.feature" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="org.codehaus.groovy.eclipse.ui"/>
   </requires>

   <plugin
         id="com.breskeby.eclipse.gradle"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
